{
  "name": "google-supercluster-map",
  "version": "1.0.0",
  "description": "google-map-react && super-cluster",
  "main": "index.js",
  "scripts": {
    "storybook": "start-storybook",
    "export-storybook": "build-storybook -c .storybook -o storybook-static",
    "dev": "NODE_ENV=development nodemon src/server/index.js",
    "start": "NODE_ENV=production node dist/server.js",
    "prebuild": "rimraf dist",
    "build": "NODE_ENV=production npm run build-client && NODE_ENV=production npm run build-server",
    "build-client": "webpack --config webpack.client.config.js --colors",
    "build-server": "webpack --config webpack.server.config.js --colors"
  },
  "author": "Matt CP Chen",
  "license": "ISC",
  "dependencies": {
    "babel-polyfill": "^6.26.0",
    "express": "^4.17.1",
    "pcln-design-system": "3.4.1",
    "react": "^16.12.0",
    "react-dom": "^16.12.0"
  },
  "devDependencies": {
    "@babel/core": "^7.4.5",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.10.1",
    "@babel/plugin-proposal-optional-chaining": "^7.10.1",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@storybook/addon-actions": "^5.3.13",
    "@storybook/react": "^5.3.13",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.0.6",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.18.3",
    "google-map-react": "1.1.7",
    "nodemon": "^1.19.1",
    "prettier": "^2.0.5",
    "rimraf": "^3.0.1",
    "styled-components": "^4.3.1",
    "supercluster": "^7.1.0",
    "use-supercluster": "^0.2.8",
    "webpack": "^4.34.0",
    "webpack-cli": "^3.3.4",
    "webpack-dev-middleware": "^3.7.0",
    "webpack-hot-middleware": "^2.25.0",
    "webpack-node-externals": "^1.7.2"
  }
